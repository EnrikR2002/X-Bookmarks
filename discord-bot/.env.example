# Discord Bot Configuration
DISCORD_TOKEN=your_discord_bot_token
DISCORD_CLIENT_ID=your_discord_application_id
GUILD_ID=your_discord_server_id

# Groq (FREE - get key at https://console.groq.com/keys)
GROQ_API_KEY=your_groq_api_key

# X/Twitter Authentication (Phase 1: Single-user)
AUTH_TOKEN=your_x_auth_token
CT0=your_x_ct0_token

# Encryption key for storing user X auth tokens (Phase 2 multi-user)
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
# Must be exactly 64 hex characters (32 bytes). DO NOT change once tokens are stored.
ENCRYPTION_KEY=generate_a_64_character_hex_string

# Logging
LOG_LEVEL=info
NODE_ENV=development
